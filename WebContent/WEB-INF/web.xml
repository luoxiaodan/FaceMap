<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <session-config>
    <session-timeout>300</session-timeout>
  </session-config>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>DbaseServlet</servlet-name>
    <servlet-class>com.stip.servlet.DbaseServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DbaseServlet</servlet-name>
    <url-pattern>/servlet/DbaseServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>GetDes</servlet-name>
    <servlet-class>com.stip.servlet.GetDes</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetDes</servlet-name>
    <url-pattern>/servlet/GetDes</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>SetDes</servlet-name>
    <servlet-class>com.stip.servlet.SetDes</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SetDes</servlet-name>
    <url-pattern>/servlet/SetDes</url-pattern>
  </servlet-mapping>

 <servlet>
    <servlet-name>FaceServlet</servlet-name>
    <servlet-class>com.stip.servlet.FaceServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FaceServlet</servlet-name>
    <url-pattern>/servlet/FaceServlet</url-pattern>
  </servlet-mapping>


</web-app>